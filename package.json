{
  "name": "antd-mobile",
  "version": "2.0.0-alpha.9",
  "description": "基于 React 的移动设计规范实现",
  "homepage": "http://mobile.ant.design",
  "keywords": [
    "ant",
    "design",
    "react",
    "react-component",
    "component",
    "components",
    "ui",
    "framework",
    "frontend",
    "mobile"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/ant-design/ant-design-mobile"
  },
  "bugs": {
    "url": "http://github.com/ant-design/ant-design-mobile/issues"
  },
  "files": [
